{{>blobfox.body-comfy}}

<g clip-path="url(#body-clip)">
    <g transform="translate(5 2)">
        {{#set-fill}}
            {{vars.marks_color}}
            | {{#base}}#left-marks{{/base}}
        {{/set-fill}}
    </g>
    <g transform="translate(-12 0)">
        {{#set-fill}}
            {{vars.marks_color}}
            | {{#base}}#right-marks{{/base}}
        {{/set-fill}}
    </g>
</g>

{{>whiskers}}
